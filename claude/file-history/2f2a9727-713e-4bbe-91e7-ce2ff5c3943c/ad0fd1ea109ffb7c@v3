pub struct Solution;

impl Solution {
    pub fn two_sum(nums: Vec<i32>, target: i32) -> Vec<i32> {
        return vec![0, 1];
    }
}

#[cfg(test)]
mod tests {
    use super::*;

    #[test]
    fn test_two_sum() {
        let tc: Vec<i32> = vec![2, 7, 11, 15];
        let tc_target: i32 = 9;
        let expected: Vec<i32> = vec![0, 1];

        let answer: Vec<i32> = Solution::two_sum(tc, tc_target);

        assert_eq!(answer, expected);
    }
}
